		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"svelte-icons": "^2.1.0",
		"svelte-icons-pack": "^2.1.0",
		"svelte-media-queries": "^1.5.4",
		"uuid": "^9.0.0"
	}
